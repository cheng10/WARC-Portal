    "test-watch": "npm test -- --watch",
    "e2e-setup": "node_modules/selenium-standalone/bin/selenium-standalone install"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel",
      "transform-regenerator",
      "transform-runtime"
    ]
  },
  "author": "Kevin Tang",
  "license": "",
  "dependencies": {
    "aphrodite": "^1.0.0",
    "connect": "3.5.0",
    "lodash": "4.16.4",
