    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "mocha": "^3.1.0",
    "nightwatch": "^0.9.8",
    "node-sass": "^3.10.1",
    "react-addons-test-utils": "^15.3.2",
