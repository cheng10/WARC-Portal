    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.3.2",
    "react-images": "^0.5.1",
    "react-photo-gallery": "^4.2.14",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.5.2",
    "redux-form": "^6.0.2",
